	private static String majors = "Accounting, Actuarial Science, Biochemistry, Biology, Bioengineering, Business, Chemistry, English, French, History, Neuroscience, Mechanical Engineering, PreMed, Psychology, Sociology";

	private static String[] girl_names = new String [1000];
	private static String[] boy_names = new String[1000];
	private static String[] last_names = new String [1000];
	private static String[] course_names = new String [1000];
	private static String[] major_names = new String [100];
	private static String[] rank_names = {"Lecturer", "Assistant Professor", "Associate Professor", "Professor"};

	
	public static void generatePersons(PersonBag theBag) {	
		storeGirlNames();
		storeBoyNames();
		storeLastNames();
		storeCourses();
		storeMajors();
		
		generateStudents(theBag);
		generateFaculty(theBag);
	}
	
	private static int nElems=0;

	public static void generateStudents(PersonBag theBag) {
		int randNum;
		String firstName;
		String lastName;
		long phoneNumber;
		double gpa;
		String major;
		String course;
		
		for(int i=0; i<1000; i++) {
			if (i%2 == 0)
				firstName = girl_names[(int)(Math.random()*girl_names.length)];
			else 
				firstName = boy_names[(int)(Math.random()*boy_names.length)];
			
			lastName = last_names[(int)(Math.random()*last_names.length)];
			phoneNumber = (long)(Math.random()*(999999999 - 000000000)) + 000000000;
			gpa = (double)(Math.random()*4);
			major = major_names[(int)(Math.random()*major_names.length)];
			course = course_names[(int)(Math.random()*course_names.length)];
			
			Student p = new Student(firstName, lastName, phoneNumber, gpa, major, course);
			theBag.insert(p);
			
			nElems++;
		}
	}
	
	public static void generateFaculty(PersonBag theBag) {
		int randNum;
		String firstName;
		String lastName;
		long phoneNumber;
		double salary;
		String rank;
		String discipline;
		
		for(int i=nElems; i<nElems+200; i++) {
			if (i%2 == 0)
				firstName = girl_names[(int)(Math.random()*girl_names.length)];
			else 
				firstName = boy_names[(int)(Math.random()*boy_names.length)];
			
			lastName = last_names[(int)(Math.random()*last_names.length)];
			phoneNumber = (long)(Math.random()*(999999999 - 000000000)) + 000000000;
			salary = (double)(Math.random()*(100000-50000)+50000);
			rank = rank_names[(int)(Math.random()*rank_names.length)];
			discipline = course_names[(int)(Math.random()*course_names.length)];
			
			Faculty p = new Faculty(firstName, lastName, phoneNumber, salary, rank, discipline);
			theBag.insert(p);
		}
	}
	
	
	//Stuff that we did before
	private static void storeGirlNames() {
		girl_names = girlNames.split("[	 ]");
	}
	
	private static void storeBoyNames() {

		for (int i=1; i<179; i++) {
			boyNames = boyNames.replace("	" + i + ". ", " ");
		}
		
		boy_names = boyNames.split(" ");
	}
	
	private static void storeLastNames() {
		last_names = lastNames.split("\n");
	}
	
	private static void storeCourses() {
		course_names = courses.split(", ");
	}
	
	private static void storeMajors() {
		major_names = majors.split(", ");
	}

}
